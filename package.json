{
  "name": "mahad-portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "backend:install": "cd backend/express && npm install",
    "backend:dev": "cd backend/express && npm run dev",
    "backend:start": "cd backend/express && npm start",
    "seed:projects": "cd backend/express && node seedProjects.js",
    "django:install": "cd backend/django && pip install -r requirements.txt",
    "django:dev": "cd backend/django && python manage.py runserver 8000",
    "full:dev": "concurrently \"npm run dev\" \"npm run backend:dev\" \"npm run django:dev\"",
    "setup": "npm install && npm run backend:install && npm run django:install",
    "init:data": "npm run seed:projects"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "lucide-react": "^0.263.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}